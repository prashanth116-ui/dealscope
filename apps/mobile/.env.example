# DealScope Mobile App Environment Variables
# Copy to .env and fill in values

# API Gateway URL (from CDK output)
EXPO_PUBLIC_API_URL=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/prod

# Cognito (from CDK output)
EXPO_PUBLIC_USER_POOL_ID=us-east-1_XXXXXXXXX
EXPO_PUBLIC_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX

# Free API Keys (register at each service)
# FRED: https://fred.stlouisfed.org/docs/api/api_key.html
EXPO_PUBLIC_FRED_API_KEY=

# Census: https://api.census.gov/data/key_signup.html
EXPO_PUBLIC_CENSUS_API_KEY=

# BLS: https://data.bls.gov/registrationEngine/
EXPO_PUBLIC_BLS_API_KEY=

# Google Geocoding (optional, for lat/lng â†’ flood zone)
# https://console.cloud.google.com/apis/credentials
EXPO_PUBLIC_GOOGLE_GEOCODING_KEY=
